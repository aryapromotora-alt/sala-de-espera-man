{
  "version": 2,
  "builds": [
    {
      "src": "api/global-session.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/global-playlists.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/global-current-playlist.py",
      "use": "@vercel/python"
    },
    {
      "src": "dist/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/global-session",
      "dest": "api/global-session.py"
    },
    {
      "src": "/api/global-playlists",
      "dest": "api/global-playlists.py"
    },
    {
      "src": "/api/global-playlists/(.*)",
      "dest": "api/global-playlists.py"
    },
    {
      "src": "/api/global-current-playlist",
      "dest": "api/global-current-playlist.py"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/index.html"
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist"
}